<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="viper">
    <static>false</static>

    <!-- Main Rocket Body -->
    <link name="base_link">

      <pose>0 0 0 0 0 0</pose>

      <velocity_decay>
        <linear>0.0</linear>
        <angular>0.0</angular>
      </velocity_decay>

      <inertial>
        <mass>1.0</mass>
        <inertia><ixx>0.008</ixx><iyy>0.008</iyy><izz>0.0005</izz></inertia>
      </inertial>

      <collision name="body_collision">
        <geometry><cylinder><radius>0.06</radius><length>0.55</length></cylinder></geometry>
        <pose>0 0 0 0 0 0</pose>
        <surface>
          <contact>
            <ode>
              <max_vel>0.1</max_vel>
              <min_depth>0.001</min_depth>
            </ode>
          </contact>
          <friction>
            <ode>
              <mu>100000.0</mu>
              <mu2>100000.0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>

      <visual name="body_visual">
        <geometry><cylinder><radius>0.03</radius><length>0.3</length></cylinder></geometry>
        <!-- <material><ambient>0.8 0.1 0.1 1</ambient></material> -->
        <material>
          <script>
            <name>Gazebo/DarkGrey</name>
          </script>
        </material>
      </visual>

      <visual name='front_left_leg_visual'>
        <pose>0.035 0 -0.20 0 -0.1745329 0</pose>
        <geometry>
          <cylinder>
            <radius>0.005</radius>
            <length>0.15</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/FlatBlack</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>

      </visual>
      <visual name='front_right_leg_visual'>
        <pose>0 0.035 -0.20 0.1745329 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.005</radius>
            <length>0.15</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/FlatBlack</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>

      <visual name='rear_left_leg_visual'>
        <pose>-0.035 0 -0.20 0 0.1745329 0</pose>
        <geometry>
          <cylinder>
            <radius>0.005</radius>
            <length>0.15</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/FlatBlack</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>

      </visual>
      <visual name='rear_right_leg_visual'>
        <pose>0 -0.035 -0.20 -0.1745329 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.005</radius>
            <length>0.15</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <name>Gazebo/FlatBlack</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>

      <!-- <sensor name="imu_sensor" type="imu">
        <pose>0 0 0 0 0 0</pose>
        <always_on>true</always_on>
        <update_rate>100.0</update_rate>
        <visualize>true</visualize>
        <imu>
          <angular_velocity><x>true</x><y>true</y><z>true</z></angular_velocity>
          <linear_acceleration><x>true</x><y>true</y><z>true</z></linear_acceleration>
        </imu>
      </sensor> -->

    </link>

    <link name='viper/imu_link'>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.15</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00002</iyy>
          <iyz>0</iyz>
          <izz>0.00002</izz>
        </inertia>
      </inertial>
      <sensor name="imu_sensor" type="imu">
        <pose>0 0 0 3.141593 0 0</pose>
        <always_on>1</always_on>
        <update_rate>1000.0</update_rate>
      </sensor>
    </link>

    <joint name='viper/imu_joint' type='revolute'>
      <child>viper/imu_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <damping>1.0</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>

    <!-- TVC Pitch Joint -->
    <joint name="tvc_pitch_joint" type="revolute">
      <parent>base_link</parent>
      <child>tvc_pitch</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.5</lower>
          <upper>0.5</upper>
          <!-- <effort>5</effort>
          <velocity>1.0</velocity> -->
        </limit>
        <!-- <dynamics>
          <damping>1.0</damping>
        </dynamics> -->
      </axis>
    </joint>

    <link name="tvc_pitch">

      <pose>0 0 -0.15 0 0 0</pose>

      <inertial>
        <mass>0.1</mass>
        <inertia><ixx>1e-5</ixx><iyy>1e-5</iyy><izz>1e-5</izz></inertia>
      </inertial>

      <!-- <visual name="body_visual">
        <geometry><cylinder><radius>0.0075</radius><length>0.075</length></cylinder></geometry>
        <material><ambient>0.8 0.1 0.1 1</ambient></material>
      </visual> -->

    </link>

    <!-- TVC Roll Joint -->
    <joint name="tvc_roll_joint" type="revolute">
      <parent>tvc_pitch</parent>
      <child>tvc_roll</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.5</lower>
          <upper>0.5</upper>
          <!-- <effort>5</effort>
          <velocity>1.0</velocity> -->
        </limit>
        <!-- <dynamics>
          <damping>1.0</damping>
        </dynamics> -->
      </axis>
    </joint>

    <link name="tvc_roll">
      <pose>0 0 -0.15 0 0 0</pose>

      <inertial>
        <mass>0.1</mass>
        <inertia><ixx>1e-5</ixx><iyy>1e-5</iyy><izz>1e-5</izz></inertia>
      </inertial>
      
      <visual name="body_visual">
        <geometry><cylinder><radius>0.0075</radius><length>0.075</length></cylinder></geometry>
        <material><ambient>0.5 0.4 0.3 1</ambient></material>
      </visual>

    </link>

    <link name='rotor_0'>

      <pose>0 0 -0.20 0 -0 0</pose>

      <inertial>
        <pose>0 0 0 0 -0 0</pose>
        <mass>0.025</mass>
        <inertia>
          <ixx>9.75e-06</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000166704</iyy>
          <iyz>0</iyz>
          <izz>0.000167604</izz>
        </inertia>
      </inertial>

      <!-- <collision name='rotor_0_collision'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <cylinder>
            <length>0.005</length>
            <radius>0.01</radius>
          </cylinder>
        </geometry>
        <surface>
          <contact>
            <ode/>
          </contact>
          <friction>
            <ode/>
          </friction>
        </surface>
      </collision> -->

      <visual name='rotor_0_visual'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.25 0.25 0.25</scale>
            <uri>model://iris_with_standoffs/meshes/iris_prop_ccw.dae</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Blue</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>

      <gravity>1</gravity>

      <velocity_decay/>

      <self_collide>0</self_collide>

    </link>

    <joint name='rotor_0_joint' type='revolute'>
      <child>rotor_0</child>
      <parent>tvc_roll</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <damping>0.004</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>

    <link name='rotor_1'>

      <pose>0 0 -0.21 0 -0 0</pose>

      <inertial>
        <pose>0 0 0 0 -0 0</pose>
        <mass>0.025</mass>
        <inertia>
          <ixx>9.75e-06</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000166704</iyy>
          <iyz>0</iyz>
          <izz>0.000167604</izz>
        </inertia>
      </inertial>

      <!-- <collision name='rotor_1_collision'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <cylinder>
            <length>0.005</length>
            <radius>0.01</radius>
          </cylinder>
        </geometry>
        <surface>
          <contact>
            <ode/>
          </contact>
          <friction>
            <ode/>
          </friction>
        </surface>
      </collision> -->

      <visual name='rotor_1_visual'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>0.25 0.25 0.25</scale>
            <uri>model://iris_with_standoffs/meshes/iris_prop_cw.dae</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/DarkGrey</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>

      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>

    </link>

    <joint name='rotor_1_joint' type='revolute'>
      <child>rotor_1</child>
      <parent>tvc_roll</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <damping>0.004</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>

  </model>
</sdf>
